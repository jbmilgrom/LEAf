        <script type="text/template" id='articles_template_received'>

          <div class="col-xs-6 col-xs-offset-3">
              <h2><a href="<%%= a_url %>"><%%= header %></a></h2>
              <p><%%= subheader %></p>
            <p>
              <a class="btn btn-default" href="#" role="button">View article »</a>
              <a class="btn btn-default" href="#" role="button">Favorite »</a>
              <a class="btn btn-default" href="#" role="button">Archive »</a>
              <% if @user == current_user %>
                <a class="btn btn-default" data-action="destroy" href="#" role="button">Delete »</a>
              <% else %>
                <a class="btn btn-default" data-action="save" href="#" role="button">Save »</a>
              <% end %>
            </p>
          </div>
        </script>

        <script type="text/template" id='articles_template_not_received'>

          <div class="col-xs-6 col-xs-offset-3">
